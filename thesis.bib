@article{Lavi2012,
abstract = {Computational classification of gene expression profiles into distinct disease phenotypes has been highly successful to date. Still, robustness, accuracy, and biological interpretation of the results have been limited, and it was suggested that use of protein interaction information jointly with the expression profiles can improve the results. Here, we study three aspects of this problem. First, we show that interactions are indeed relevant by showing that co-expressed genes tend to be closer in the network of interactions. Second, we show that the improved performance of one extant method utilizing expression and interactions is not really due to the biological information in the network, while in another method this is not the case. Finally, we develop a new kernel method--called NICK--that integrates network and expression data for SVM classification, and demonstrate that overall it achieves better results than extant methods while running two orders of magnitude faster.},
author = {Lavi, Ofer and Dror, Gideon and Shamir, Ron},
doi = {10.1089/cmb.2012.0065},
file = {:C$\backslash$:/Users/npfeifer/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Lavi, Dror, Shamir - 2012 - Network-induced classification kernels for gene expression profile analysis.pdf:pdf},
issn = {1557-8666},
journal = {J Comput Biol.},
keywords = {Algorithms,Artificial Intelligence,Computational Biology,Computational Biology: methods,Gene Expression,Gene Expression Profiling,Gene Expression Profiling: classification,Gene Expression Profiling: methods,Gene Regulatory Networks,Humans,Software},
month = jun,
number = {6},
pages = {694--709},
pmid = {22697242},
title = {{Network-induced classification kernels for gene expression profile analysis.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3375644\&tool=pmcentrez\&rendertype=abstract},
